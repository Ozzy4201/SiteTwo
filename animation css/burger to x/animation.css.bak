.Top {
	position: absolute;
	width: 80px;
	height: 15px;
	left: 0px;
	top: 0px;
	margin: 10px;
	background: #000000;
	box-shadow: 0px 6px 6px rgba(0, 0, 0, 0.25);
	border-radius: 4px;
}
.Middle {
	position: absolute;
	width: 80px;
	height: 15px;
	left: 0;
	top: 25px;
	margin: 10px;
	background: #000000;
	box-shadow: 0px 6px 6px rgba(0, 0, 0, 0.25);
	border-radius: 4px;
}
.Bottom {
	position: absolute;
	width: 80px;
	height: 15px;
	left: 0px;
	top: 50px;
	margin: 10px;
	background: #000000;
	box-shadow: 0px 6px 6px rgba(0, 0, 0, 0.25);
	border-radius: 4px;
}

#Bottom {
    transform: translateX(-50%) translateY(-50%);
    animation: Bottom__y__1 500ms 0ms ease-in-out forwards, Bottom__rotation__1 500ms 0ms ease-in-out forwards;
}

@keyframes Bottom__y__1 {
    from {
        top: 57.5px;
    }

    to {
        top: 32.5px;
    }
}
@keyframes Bottom__rotation__1 {
    from {
        transform: translateX(-50%) translateY(-50%) rotate(0deg);
    }

    to {
        transform: translateX(-50%) translateY(-50%) rotate(-45deg);
    }
}

#Middle {
    transform: translateX(-50%) translateY(-50%);
    animation: Middle__opacity__1 70ms 0ms ease-out forwards;
}

@keyframes Middle__opacity__1 {
    from {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

#Top {
    transform: translateX(-50%) translateY(-50%);
    animation: Top__y__1 500ms 0ms ease-in-out forwards, Top__rotation__1 500ms 0ms ease-in-out forwards;
}

@keyframes Top__y__1 {
    from {
        top: 7.5px;
    }

    to {
        top: 32.5px;
    }
}
@keyframes Top__rotation__1 {
    from {
        transform: translateX(-50%) translateY(-50%) rotate(0deg);
    }

    to {
        transform: translateX(-50%) translateY(-50%) rotate(45deg);
    }
}
